{
  "name": "react-animated-dropdown",
  "version": "1.1.3",
  "private": false,
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "description": "An Alternative to default html select tag with animated select flow and customizable ui",
  "main": "dist/custom-input-select.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "publish:npm": "rm -rf dist && mkdir dist &&  babel ./src -d dist --copy-files",
    "build": "npx babel src --out-dir dist",
    "start": "npx babel src --watch --out-dir dist"
  },
  "keywords": [
    "react-animated-dropdown",
    "dropdown",
    "select",
    "react-select",
    "react-dropdown",
    "react-custom-select"
  ],
  "author": "Kushagra Madhukar",
  "license": "MIT",
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "@material-ui/icons": "^4.11.2",
    "styled-components": "^5.2.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/plugin-transform-react-jsx": "^7.13.12",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13"
  }
}
